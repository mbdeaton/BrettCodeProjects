Cell[BoxData[
 RowBox[{
  RowBox[{"Vnuierg", "[", 
   RowBox[{"fnu_", ",", 
    RowBox[{"(*", " ", 
     RowBox[{"array", " ", "of", " ", "data"}], " ", "*)"}], 
    "\[IndentingNewLine]", "qMinMeV_", ",", "qMaxMeV_", ",", 
    RowBox[{"(*", " ", 
     RowBox[{
      SubscriptBox["p", "t"], " ", "bounds"}], " ", "*)"}], 
    "\[IndentingNewLine]", "cosAMin_", ",", "cosAMax_"}], 
   RowBox[{"(*", " ", 
    RowBox[{"cosA", " ", "bounds"}], " ", "*)"}], "\[IndentingNewLine]", 
   "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Block", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Nq", "=", "numqBins"}], ",", "\[IndentingNewLine]", 
      RowBox[{"NA", "=", "numcosABins"}], ",", "\[IndentingNewLine]", 
      RowBox[{"NB", "=", "numBBins"}], ",", "\[IndentingNewLine]", 
      RowBox[{"\[CapitalDelta]q", "=", 
       FractionBox[
        RowBox[{"qMaxMeV", "-", "qMinMeV"}], "Nq"]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"\[CapitalDelta]cosA", "=", 
       FractionBox[
        RowBox[{"cosAMax", "-", "cosAMin"}], "NA"]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"\[CapitalDelta]B", "=", 
       RowBox[{"If", "[", 
        RowBox[{"assumeBSymmetry", ",", 
         FractionBox["\[Pi]", "NB"], ",", 
         FractionBox[
          RowBox[{"2", "\[Pi]"}], "NB"]}], "]"}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"returnArray", "=", 
       RowBox[{"ConstantArray", "[", 
        RowBox[{
         RowBox[{"{", "}"}], ",", "numqBins"}], "]"}]}]}], "}"}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Depth", "[", "fnu", "]"}], "\[NotEqual]", "5"}], ",", 
       RowBox[{
        RowBox[{"Message", "[", 
         RowBox[{"Vnuierg", "::", "argdeptherr"}], "]"}], ";", 
        RowBox[{"Abort", "[", "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "fnu", "]"}], "\[NotEqual]", "Nq"}], ",", 
       RowBox[{
        RowBox[{"Message", "[", 
         RowBox[{"Vnuierg", "::", "arglengtherr"}], "]"}], ";", 
        RowBox[{"Abort", "[", "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", 
         RowBox[{"fnu", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "]"}], "\[NotEqual]", "NA"}], ",", 
       RowBox[{
        RowBox[{"Message", "[", 
         RowBox[{"Vnuierg", "::", "arglengtherr"}], "]"}], ";", 
        RowBox[{"Abort", "[", "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", 
         RowBox[{"fnu", "[", 
          RowBox[{"[", 
           RowBox[{"1", ",", "1"}], "]"}], "]"}], "]"}], "\[NotEqual]", 
        "NB"}], ",", 
       RowBox[{
        RowBox[{"Message", "[", 
         RowBox[{"Vnuierg", "::", "arglengtherr"}], "]"}], ";", 
        RowBox[{"Abort", "[", "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", "Nq"}], ",", 
       RowBox[{"++", "i"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"returnArray", "[", 
         RowBox[{"[", "i", "]"}], "]"}], "=", 
        RowBox[{"Sum", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Block", "[", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"q", "=", 
               RowBox[{"fnu", "[", 
                RowBox[{"[", 
                 RowBox[{"i", ",", "j", ",", "k", ",", "1"}], "]"}], "]"}]}], 
              ",", "\[IndentingNewLine]", 
              RowBox[{"Cos\[CapitalTheta]", "=", 
               RowBox[{"fnu", "[", 
                RowBox[{"[", 
                 RowBox[{"i", ",", "j", ",", "k", ",", "2"}], "]"}], "]"}]}], 
              ",", "\[IndentingNewLine]", 
              RowBox[{"f", "=", 
               RowBox[{"fnu", "[", 
                RowBox[{"[", 
                 RowBox[{"i", ",", "j", ",", "k", ",", "3"}], "]"}], 
                "]"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
            RowBox[{
             SuperscriptBox["q", "2"], 
             RowBox[{"(", 
              RowBox[{"1", "-", "Cos\[CapitalTheta]"}], ")"}], "f"}]}], 
           "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"j", ",", "1", ",", "NA"}], "}"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"k", ",", "1", ",", "NB"}], "}"}]}], "\[IndentingNewLine]",
          "]"}]}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"returnArray", "*=", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{"assumeBSymmetry", ",", "2", ",", "1"}], "]"}], "*", 
       FractionBox[
        RowBox[{
         SqrtBox["2"], "GFNat"}], 
        RowBox[{"8", 
         SuperscriptBox["\[Pi]", "3"]}]], "*", "\[CapitalDelta]q", "*", 
       "\[CapitalDelta]cosA", "*", "\[CapitalDelta]B", "*", 
       "MeVToerg"}]}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input"]
}, Open  ]],

